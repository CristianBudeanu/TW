@model List<EasyBreath.Domain.Entities.User.UDbModel>
@using EasyBreath.web.Extensions

@{
     Layout = null;
}



<table border="1" cellspacing="0">
     <tr>
          <th>
               @Html.DisplayNameFor(model => model[0].Username)
          </th>
          <th>
               @Html.DisplayNameFor(model => model[0].Email)
          </th>
          <th></th>
     </tr>

     @foreach (var item in Model)
     {
          <tr>
               <td>
                    @Html.DisplayFor(modelItem => item.Username)
               </td>
               <td>
                    @Html.DisplayFor(modelItem => item.Email)
               </td>
               <td>
                    @Html.DisplayFor(modelItem => item.AccessLevel)
               </td>
               @if (System.Web.HttpContext.Current.GetMySessionObject().Level == EasyBreath.Domain.Enum.URole.ADMINISTRATOR)
               { 
                    <td>
                        
                              @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                              @Html.ActionLink("Delete", "Delete", new { id = item.Id })
               
                    </td>
               }
          </tr>
     }

</table>




